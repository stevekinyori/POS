<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SupplierProducts.
    -->
    <changeSet id="20171201143250-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="supplier_products"
                                 constraintName="fk_supplier_products_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="products"/>

        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="supplier_products"
                                 constraintName="fk_supplier_products_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suppliers"/>

    </changeSet>
</databaseChangeLog>
